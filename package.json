{
    "name": "foxit-pdf-skd-for-web-examples-catalog",
    "version": "0.0.1",
    "description": "Foxit pdf sdk for web.",
    "author": "Foxit Software Inc.",
    "main": "./lib/UIExtension.full.js",
    "types": "./lib/UIExtension.d.ts",
    "scripts": {
        "preinstall": "npm i -S @foxitsoftware/foxit-pdf-sdk-for-web-library-full",
        "move-sdk-assets": "cp-cli ./node_modules/@foxitsoftware/foxit-pdf-sdk-for-web-library-full/lib ./dist/lib",
        "start": "concurrently --kill-others \"npm run start:webpack\" \"npm run start:snapshot-server\" \"npm run start:collaboration-server\"",
        "start:webpack": "webpack serve --config ./build/webpack.config.js --mode development",
        "start:snapshot-server": "node ./node_modules/@foxitsoftware/foxit-pdf-sdk-for-web-library-full/server/snapshot/src/index -p 3003",
        "start:collaboration-server": "cross-env SERVER_PORT=19112 node ./examples/collaboration/sample-server/server.js",
        "build": "npm run move-sdk-assets && webpack --config ./build/webpack.config.js --mode production",
        "ciServer": "concurrently \"npm run start:snapshot-server\" \"npm run start:collaboration-server\""
    },
    "config": {
        "unsafe-perm": true
    },
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/plugin-transform-runtime": "^7.13.10",
        "@babel/preset-env": "^7.13.10",
        "@babel/preset-react": "^7.12.13",
        "@babel/preset-typescript": "^7.13.0",
        "@babel/runtime": "^7.13.10",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.2",
        "@types/react-router": "^5.1.12",
        "art-template": "^4.13.2",
        "art-template-loader": "^1.4.3",
        "babel-loader": "^8.2.2",
        "boxen": "^4.1.0",
        "chalk": "^2.4.1",
        "concurrently": "^5.3.0",
        "copy-webpack-plugin": "^6.3.2",
        "cp-cli": "^2.0.0",
        "cross-env": "^7.0.2",
        "css-loader": "^5.0.1",
        "file-loader": "^6.2.0",
        "formidable": "^1.2.2",
        "fs-extra": "^9.0.1",
        "html-loader": "^1.3.2",
        "html-webpack-plugin": "^4.5.0",
        "http-proxy-middleware": "^0.19.1",
        "koa": "^2.7.0",
        "koa-body": "^4.0.4",
        "koa-body-parser": "^1.1.2",
        "koa-router": "^7.4.0",
        "koa2-connect": "^1.0.2",
        "less": "^4.1.1",
        "less-loader": "^8.0.0",
        "lru-cache": "^4.1.3",
        "mini-css-extract-plugin": "^1.3.1",
        "node-cmd": "^5.0.0",
        "pm2": "^5.2.0",
        "process": "^0.11.10",
        "raw-body": "^2.3.3",
        "require-dir": "^1.0.0",
        "rimraf": "^3.0.2",
        "serve-handler": "^6.0.2",
        "sockjs-client": "^1.6.1",
        "style-loader": "^2.0.0",
        "url-loader": "^4.1.1",
        "webpack": "^5.9.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0"
    },
    "serve": {
        "port": 8083,
        "proxy": {
            "/snapshot": {
                "target": "http://127.0.0.1:3003",
                "changeOrigin": true
            },
            "/collab": {
                "target": "http://127.0.0.1:19112",
                "changeOrigin": true,
                "ws": true
            }
        }
    },
    "dependencies": {
        "@foxitsoftware/collab-db": "file:../CollaborationAddon/packages/collab-db",
        "@foxitsoftware/collab-server": "file:../CollaborationAddon/packages/web-collab-server",
        "@foxitsoftware/foxit-pdf-sdk-for-web-library-full": "^8.5.0",
        "@foxitsoftware/web-collab-client": "file:../CollaborationAddon/packages/web-collab-client",
        "@types/react-router-dom": "^5.1.7",
        "antd": "^4.14.0",
        "axios": "^0.19.0",
        "copy-to-clipboard": "^3.3.1",
        "core-js": "^3.9.1",
        "driver.js": "^0.9.8",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0"
    }
}
